# Database Configuration
DB_USERNAME=ltuser
DB_PASSWORD=your_secure_database_password_here

# JWT Configuration
JWT_SECRET_KEY=your_secure_jwt_secret_key_here_minimum_256_bits

# IP Check Service
IP_CHECK_SECRET_KEY=your_secure_ip_check_secret_key_here

# Swagger Authentication (Production)
SWAGGER_USERNAME=admin
SWAGGER_PASSWORD=your_secure_swagger_password_here

# AbuseIPDB API (if needed)
ABUSE_API_KEY=your_abuseipdb_api_key_here

# Email Configuration (if needed)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_email_app_password_here

# Telegram Bot (if needed)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Server Configuration
SERVER_PORT=1999
SERVER_ADDRESS=127.0.0.1

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=/var/log/ltapp/application.log

# Cache Configuration
CACHE_MAX_SIZE=500
CACHE_EXPIRE_AFTER_ACCESS=600

# Rate Limiting
RATE_LIMIT_MAX_ATTEMPTS=5
RATE_LIMIT_DURATION=300000